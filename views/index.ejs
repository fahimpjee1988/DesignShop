<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="/style.css" />
  </head>
  <body>
    <nav>
      <div>
        <h3>The Design Shop</h3>
      </div>
    </nav>
    <div>
      <section id="gallery">
        <div>
          <% gallery.forEach(element => { %>
          <img src="<%= element.Product_pictureLink %> " alt="" />
          <h3><%= element.product_name %></h3>
          <h4><%= element.Price %>$</h4>
          <a href="<%= element.Link_shop %> ">BUY</a>
          <% }) %>
        </div>
      </section>
    </div>
  </body>
</html>
